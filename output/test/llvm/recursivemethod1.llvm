; ModuleID = 'Test'

declare void @print_int(i32)

define i32 @test(i32 %x) {
entry:
  %y = alloca i32
  %x1 = alloca i32
  store i32 10, i32* %y
  ret i32 10
}

define i32 @main() {
entry:
  %x = alloca i32
  store i32 1, i32* %x
  %x1 = load i32, i32* %x
  %calltmp = call i32 @test(i32 %x1)
  call void @print_int(i32 %calltmp)
  ret i32 0
}
